{
	"name" : "Platypus",
	"id" : "com.gopherwoodstudios.platypus",
	"version" : "0.3.1",
	"github": "Gopherwood/PlatypusSpringRollTemplate",
	"rename" : {
		"gitignore" : ".gitignore",
		"bowerrc" : ".bowerrc",
		"project.md": "README.md",
		"bower": "bower.json"
	},
	"extend": "io.springroll.default",
	"required": ["core", "platypus"],
	"modules": [
		{
			"id": "platypus",
			"name": "Platypus Game Engine",
			"description": "An HTML5 2D tile-based game framework.",
			"main": [
				"components/platypus/lib/platypus-0.3.1.min.js",
				"components/platypus/lib/platypus-0.3.1.min.css"
			],
			"mainDebug": [
				"components/platypus/lib/platypus-0.3.1.combined.js",
				"components/platypus/lib/platypus-0.3.1.combined.css"
			],
			"bower": {
				"platypus": "https://github.com/gopherwood/Platypus.git#simplified"
			},
			"libraries": [
				"components/easeljs/lib/easeljs-0.8.1.min.js",
				"components/preloadjs/lib/preloadjs-0.6.1.min.js",
				"components/soundjs/lib/soundjs-0.6.1.min.js",
				"components/soundjs/lib/flashaudioplugin-0.6.1.min.js",
				"components/tweenjs/lib/tweenjs-0.6.1.min.js"
			],
			"librariesDebug": [
				"components/easeljs/lib/easeljs-0.8.1.combined.js",
				"components/preloadjs/lib/preloadjs-0.6.1.combined.js",
				"components/soundjs/lib/soundjs-0.6.1.combined.js",
				"components/soundjs/lib/flashaudioplugin-0.6.1.combined.js",
				"components/tweenjs/lib/tweenjs-0.6.1.combined.js"
			],
			"librariesCopy": {
				"components/soundjs/src/soundjs/FlashAudioPlugin.swf": "deploy/assets/swfs/"
			},
			"default": true
		}
	]
}
